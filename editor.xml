<application>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *.erb" />
    <option name="SHOW_INTENTION_PREVIEW" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="enabled" value="false" />
    <option name="lastViewedProviderKeyId" value="TypeScript.ts.type.hints" />
    <settingsMapElement>
      <JavaScript.js.type.hints>
        <Settings>
          <option name="showForFunctions" value="true" />
          <option name="showForShorthandFunctions" value="true" />
          <option name="showForVariablesAndFields" value="true" />
        </Settings>
      </JavaScript.js.type.hints>
      <kotlin.kotlin.references.types.hints>
        <Settings>
          <option name="functionReturnType" value="true" />
        </Settings>
      </kotlin.kotlin.references.types.hints>
      <TypeScript.ts.type.hints>
        <Settings>
          <option name="showForFunctions" value="true" />
          <option name="showForShorthandFunctions" value="true" />
          <option name="showForVariablesAndFields" value="true" />
        </Settings>
      </TypeScript.ts.type.hints>
      <JAVA.annotation.hints>
        <Settings>
          <option name="showInferred" value="true" />
        </Settings>
      </JAVA.annotation.hints>
      <kotlin.kotlin.lambdas.hints>
        <Settings>
          <option name="implicitReceiversAndParams" value="false" />
        </Settings>
      </kotlin.kotlin.lambdas.hints>
    </settingsMapElement>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>